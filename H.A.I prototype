	window.onpageshow = function(evt) {
		var basketTarget = $(".sub_category_btn .cart_num").size() > 0 ? $(".sub_header_area .cart_num") : $(".gs-wrap .go-cart .cnt");
		var basketFlag = basketTarget.attr("data-flag");
		var basketCnt = getCmmCookie("cartcnt");

		if (basketFlag != undefined && basketFlag == "Y" && basketCnt != 0) {
			if (basketCnt > 99) {
				basketCnt = 99;
			}
			basketTarget.html(basketCnt);
			basketTarget.show();
		}
		else {
			basketTarget.html(0);
			basketTarget.hide();
		}
	}

	//앱에서 호출
	function refreshBasketCnt() {

		var basketCnt = getCmmCookie("cartcnt");
		var basketTarget = $(".sub_category_btn .cart_num").size() > 0 ? $(".sub_header_area .cart_num") : $(".gs-wrap .go-cart .cnt");
		if (basketCnt != 0) {
			if (basketCnt > 99) {
				basketCnt = 99;
			}
			basketTarget.html(basketCnt);
			basketTarget.show();
		}
		else {
			basketTarget.html(0);
			basketTarget.hide();
		}
	}

	function getCmmCookie(c_name) {
		var i, x, y, ARRcookies = document.cookie.split(";");
		for (i = 0; i < ARRcookies.length; i++) {
			x = ARRcookies[i].substr(0, ARRcookies[i].indexOf("="));
			y = ARRcookies[i].substr(ARRcookies[i].indexOf("=") + 1);
			x = x.replace(/^\s+|\s+$/g, "");
			if (x == c_name) {
				return unescape(y);
			}
		}
		return "0";
	}


<script>
document.addEventListener('DOMContentLoaded', function(){
	// common.min.js 추가 로딩
	if (!gs.modal || gs.modal == undefined){    
		const srchJS  = [
			'jquery.min.js',
			'jquery-1.8.2.min.js',
			'swiper.js',
			'swiper.min.js',
			'idangerous.swiper-2.7.6.js',
			'video.min.js'
		];
		let scripts = document.querySelectorAll('script');
		let dependencyTarget;
		scripts.forEach((obj)=>{
			for (var value of srchJS) {
				if (obj.src && obj.src.indexOf( value ) != -1){
					dependencyTarget = obj;
				}
			}
		});
		let commonJs = document.createElement("script");
		commonJs.type = "text/javascript";
		commonJs.src = "//public.m-gs.kr/ui/gsshop/mc/assets/scripts/common.min.js?20240701134200";
		dependencyTarget.after(commonJs);
	}
}, true);
</script>


